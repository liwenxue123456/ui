<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.qf.dao.DoctorDao">

    <select id="selectByDeptNo" resultType="Doctor">
        select did, dname
        from doctor
        where deptNo = #{deptNo}
    </select>

</mapper>